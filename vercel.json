{
  "version": 2,
  "name": "coloreveryday",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/generated-images/(.*)",
      "dest": "/generated-images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite",
  "env": {
    "VITE_GEMINI_API_KEY": "@vite_gemini_api_key"
  }
}